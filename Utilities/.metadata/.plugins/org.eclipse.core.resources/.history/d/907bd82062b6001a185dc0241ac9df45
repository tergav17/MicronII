package asm;

public class Numeric {
	private int value;
	private int type;
	
	public Numeric(int value, int type) {
		this.value = value;
		this.type = type;
	}
	
	public int getValue() {
		return value;
	}
	
	public void setValue(int value) {
		this.value = value;
	}
	
	public int getType() {
		return type;
	}
	
	public void setType(int type) {
		this.type = type;
	}
	
	
}
